{
  "name": "PHub's Age Verification Remover",
  "description": "Removes age verification",
  "version": "1.3.4",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "logo.png"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.pornhub.org/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
